    "babel-plugin-component": "^1.1.1",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.12",
    "cross-env": "^6.0.3",
    "eslint-plugin-cypress": "^2.0.1",
    "flow-bin": "^0.129.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "mockjs": "^1.1.0",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
