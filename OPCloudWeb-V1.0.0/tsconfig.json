{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "jsx": "preserve",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": "./",
    "paths": {
      "@/plugins/*": ["src/plugins/*"],
      "@/views/*": ["src/views/*"],
      "@/utils/*": ["src/utils/*"],
      "@/router/*": ["src/router/*"],
      "@/use/*": ["src/use/*"],
      "@/styles/*": ["src/styles/*"],
      "@/config/*": ["src/config/*"],
      "@/layout/*": ["src/layout/*"],
      "@/interfaces/*": ["src/interfaces/*"],
      "@/components/*": ["src/components/*"],
      "@/api/*": ["src/api/*"],
      "@/viewHooks/*": ["src/viewHooks/*"],
      "@/directives/*": ["src/directives/*"]
    },
    "types": ["node", "./src/types", "element-plus/global"],
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/**/*.js",
    "./src/**/*.vue",
    "public/js/jq-3.31.js"
  ],
  "exclude": ["node_modules", "dist"]
}
